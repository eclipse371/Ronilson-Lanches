
  <h1>Editar Acompanhamento</h1>
<a href="/logout" class="logout-button">Sair</a>

  <form>
      <label for="nome">Nome:</label>
      <input type="text" id="nome" name="nome" value="<%= acompanhamento.nome %>" required />

      <label for="preco">Preço:</label>
      <input type="number" step="0.01" id="preco" name="preco" value="<%= acompanhamento.preco %>" required />

      <button id="btn-editar" data-id="<%= acompanhamento.id %>" type="button">Salvar Alterações</button>
  </form>

